{
  "name": "lord97/laravel-llm-translator",
  "description": "Cache-first, DB-backed translations for Laravel using modular LLM drivers.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "translations", "llm", "gemini", "localization", "package"],
  "homepage": "https://github.com/lord97/laravel-llm-translator",
  "authors": [
    {
      "name": "Mohamed Bachir Sanou",
      "email": "sanoumohamed97@gmail.com",
      "homepage": "https://lord97.github.io/my-portfolio/"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^10.0",
    "illuminate/support": "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Lord97\\LaravelLlmTranslator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lord97\\LaravelLlmTranslator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lord97\\LaravelLlmTranslator\\LaravelLlmTranslatorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit",
    "lint": "pint"
  }
}