  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelLlm\\Translator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelLlm\\Translator\\TranslatorServiceProvider"
      ]
    }
  }
}
